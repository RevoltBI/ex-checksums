	<script type="text/javascript">
	//<![CDATA[

	$("#zip").autocomplete({
		source: function (request, response) {

			jQuery.get("//www.autobazar.eu/ajax/zip.php", {
				query: request.term
			}, function (data) {
				// assuming data is a JavaScript array such as
				// ["one@abc.de", "onf@abc.de","ong@abc.de"]
				// and not a string
				 data = JSON.parse(data);
				response(data);
			});
		},
		appendTo: "#zipform",
 
		select: function(event, ui){
            if (ui.item && ui.item.value){
                titleinput = ui.item.value;
                ui.item.value= ui.item.value.substring(0, 5);
				 $('#countries').hide();  $('#kmvzdialenost').show();
            } 
        },
		minLength: 1,
		
	});
 
	//]]>
	</script>
</span>  
			
