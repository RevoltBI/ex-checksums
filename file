	<label>Vzdialenosť (max km):</label>
	<input name="kms" id="kms" type="text" style="width:214px;" value="15" size="3"  maxlength="3" onkeypress="return numbersonly(this, event)" onkeyup="ajaxFunctionSerialize();">
