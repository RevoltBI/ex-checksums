	function SetCookiemsg(cookieName,cookieValue,nDays) {
	 var today = new Date();
	 var expire = new Date();
	 if (nDays==null || nDays==0) nDays=1;
	 expire.setTime(today.getTime() + 3600000*24*nDays);  
	 document.cookie = cookieName+"="+escape(cookieValue)+";path=/"+";domain=.autobazar.eu"+";expires="+expire.toGMTString();
	 }	
	 function cookiemsg(){$("#cookieinfo").toggle(); SetCookiemsg('cookieMsg',1,365); $("#biglink").css({'margin-top':'0px'}); $("#m_top").css("top", 0); $("#main-i").css("padding-top", 0); $(".n-box-mini").css("cssText", "top: 2px !important;");}
