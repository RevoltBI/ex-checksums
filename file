//<![CDATA[
function multify(){
	
	$("#BrandID").multiselect({
		checkAllText: '',
		uncheckAllText: 'Resetovať',
		noneSelectedText: 'všetky značky',
		selectedText: 'Vybrané: #',
		open: function () { $("input[type='search']").focus(); }, 
		uncheckAll:function () { $('#znackacounter').html(''); clearModel(document.searcher); $("#CarModelID").multiselect("refresh"); $(this).multiselect("widget").find("input:checkbox:not(:checked)").prop( "disabled", false );},
		click: function(e){
		$('#znackacounter').html(' '+$(this).multiselect("widget").find("input:checked").length+'/5');
        if( $(this).multiselect("widget").find("input:checked").length >=5 ){
           //alert("You can only check two checkboxes!");
		   $(this).multiselect("widget").find("input:checkbox:not(:checked)").prop( "disabled", true );
		   if($(this).multiselect("widget").find("input:checked")>5)
           return false;
		   else 
		   return true;
		   } else {
			   $(this).multiselect("widget").find("input:checkbox:not(:checked)").prop( "disabled", false );
		   }
		  // chooseModel(document.searcher);
	    },
		minWidth:246,
		selectedList: 3 
	}).multiselectfilter({
      label: '',
      placeholder: 'hľadať značku',
      autoReset:true, 
    }
	);
	$("#BrandID").multiselect("widget").find("a.ui-multiselect-close").append('✓ Potvrdiť');
