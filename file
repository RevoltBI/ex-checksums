 var _oneall = _oneall || [];
 _oneall.push(['social_login', 'set_callback_uri', '//www.autobazar.eu/sociallogin.php']);
 _oneall.push(['social_login', 'set_providers', ['google', 'twitter']]);
 _oneall.push(['social_login', 'do_render_ui', 'oa_social_login_container']);
window.oneallloaded=0;

function one(){
$("iframe[id*='oa_social_login_frame']").css('width','100%');
	if(window.oneallloaded==0){
		var oneall_subdomain = 'autobazareu';
		/* The library is loaded asynchronously */
		var oa = document.createElement('script');
		oa.type = 'text/javascript'; oa.async = true;
		oa.src = '//' + oneall_subdomain + '.api.oneall.com/socialize/library.js';
		var s = document.getElementsByTagName('script')[0];
		s.parentNode.insertBefore(oa, s);
		window.oneallloaded=1
	}
}
</script>
